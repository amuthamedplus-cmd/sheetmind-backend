# App
APP_ENV=production
DEBUG=false

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis
REDIS_URL=redis://localhost:6379/0

# OpenRouter (single key for all AI models)
OPENROUTER_API_KEY=

# Google Gemini API (direct, for embeddings and optional direct LLM)
GEMINI_API_KEY=
GEMINI_ENABLED=false

# LangChain Agent Settings
LANGCHAIN_ENABLED=true
RAG_ENABLED=true
CHROMA_PERSIST_DIR=./chroma_db
RAG_THRESHOLD_ROWS=500
RAG_RESULTS_COUNT=30
MEMORY_WINDOW_SIZE=10

# PII Detection â€” warns users when sensitive data is sent to LLM APIs
PII_DETECTION_ENABLED=true

# Dodo Payments
DODO_PAYMENTS_API_KEY=
DODO_PAYMENTS_WEBHOOK_KEY=
DODO_PAYMENTS_ENVIRONMENT=test_mode
DODO_PRO_MONTHLY_PRODUCT_ID=
DODO_PRO_ANNUAL_PRODUCT_ID=
DODO_TEAM_MONTHLY_PRODUCT_ID=
DODO_TEAM_ANNUAL_PRODUCT_ID=

# CORS
CORS_ORIGINS=http://localhost:3000,https://docs.google.com

# Frontend URL (redirect after Google OAuth)
FRONTEND_URL=http://localhost:3000
